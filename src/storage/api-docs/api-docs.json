                    "title": {
                        "type": "string",
                        "example": "The Great Gatsby"
                    },
                    "author": {
                        "type": "string",
                        "example": "F. Scott Fitzgerald"
                    },
                    "status": {
                        "type": "string",
                        "example": "completed"
                    },
                    "categories": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "currentPage": {
                        "type": "string",
                        "example": "100"
                    },
                    "cover": {
                        "type": "string",
                        "example": "url_to_cover_image"
                    },
                    "description": {
                        "type": "string",
                        "example": "A novel about the American dream."
                    },
                    "pageCount": {
                        "type": "string",
                        "example": "200"
                    },
                    "publishedDate": {
                        "type": "string",
                        "example": "1925-04-10"
                    },
                    "rating": {
                        "type": "string",
